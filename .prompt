# Prompt
    set     red="%{\033[1;31m%}"
    set   green="%{\033[0;32m%}"
    set  yellow="%{\033[1;33m%}"
    set    blue="%{\033[1;34m%}"
    set magenta="%{\033[1;35m%}"
    set    cyan="%{\033[1;36m%}"
    set   white="%{\033[0;37m%}"
    set     end="%{\033[0m%}"    # This is needed at the end... :(

    set prompt='[%n@%m %B%c%b]%# '
    set ellipsis
    set prompt = "-(${cyan}%B%P%b${end} ${green}%B%/%b${end})-\n#${end} "
