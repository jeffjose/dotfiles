{
  "editor.renderLineHighlight": "none",
  "editor.cursorBlinking": "blink",
  "editor.fontFamily": "'monospace', monospace, 'Droid Sans Fallback'",
  "editor.autoClosingBrackets": "never",
  "editor.autoClosingQuotes": "never",
  "vim.incsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],
  "vim.insertModeKeyBindingsNonRecursive": [
    {
      "before": ["(", ")"],
      "after": ["(", ")", "<Left>"]
    },
    {
      "before": ["[", "]"],
      "after": ["[", "]", "<Left>"]
    },
    {
      "before": ["<", ">"],
      "after": ["<", ">", "<Left>"]
    },
    {
      "before": ["{", "}"],
      "after": ["{", "}", "<Left>"]
    },
    {
      "before": ["\"", "\""],
      "after": ["\"", "\"", "<Left>"]
    },
    {
      "before": ["'", "'"],
      "after": ["'", "'", "<Left>"]
    }
  ],
  "vim.otherModesKeyBindingsNonRecursive": [
    {
      "before": ["<Space>"],
      "after": [],
      "commands": [
        {
          "command": ":nohl",
          "args": []
        }
      ]
    }
  ],
  "extensions.ignoreRecommendations": false,
  "workbench.startupEditor": "newUntitledFile",
  "workbench.iconTheme": "material-icon-theme",
  "prettier.disableLanguages": ["python"],
  "editor.formatOnSave": true,
  "vim.hlsearch": true,
  "python.formatting.provider": "black",
  "vim.commandLineModeKeyBindings": [],
  "editor.cursorStyle": "block",
  "workbench.colorTheme": "Material-Theme-jeffjose",
  "editor.formatOnType": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[svelte]": {
    "editor.defaultFormatter": "JamesBirtles.svelte-vscode"
  },
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2,
    "editor.formatOnType": true,
    "editor.defaultFormatter": "black"
  },
  "editor.tabSize": 2,
  "editor.minimap.enabled": false,
  "diffEditor.ignoreTrimWhitespace": true,
  "explorer.openEditors.visible": 0,
  "workbench.editor.showTabs": false,
  "explorer.confirmDelete": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "git.autofetch": true
}
